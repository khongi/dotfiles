#====== Oh My Zsh
export ZSH=$HOME/.oh-my-zsh

# Source common config
source $HOME/custom/sources/zsh_common.sh
source $HOME/custom/sources/powerlevel9k_common.sh

# Override common config
ZSH_THEME="robbyrussell"

plugins=(
  git zsh-nvm zsh-autosuggestions zsh-syntax-highlighting
)

autoload -U compinit && compinit
source $ZSH/oh-my-zsh.sh

#====== Other configuration

INJECTED=$HOME/custom/scripts

export PATH=$INJECTED:$PATH
